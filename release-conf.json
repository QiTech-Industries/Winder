{
    "categories": [
        {
            "title": "## 🚀 Features",
            "labels": [
                ""
            ]
        },
        {
            "title": "## 🐛 Fixes",
            "labels": [
                "fix"
            ]
        },
        {
            "title": "## 🧪 Tests",
            "labels": [
                "test"
            ]
        }
    ],
    "empty_template": "❌ **Whoops** there seem to be no changes in this release.",
    "template": "${{CHANGELOG}}\n\n<details>\n<summary>Uncategorized</summary>\n\n${{UNCATEGORIZED}}\n</details>",
    "pr_template": "${{AUTHOR}}",
    "sort": "DESC",
    "label_extractor": [
        {
            "pattern": "(.) (.+)",
            "target": "$1",
            "flags": "gu"
        },
        {
            "pattern": "\\[Issue\\]",
            "on_property": "title",
            "method": "match"
        }
    ]
}